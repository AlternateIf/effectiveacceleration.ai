  "scripts": {
    "clean:contracts": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "build": "yarn prettier '**/*.sol' --write && yarn clean:contracts && yarn compile",
    "test": "npx hardhat test",
    "size": "npx hardhat size-contracts",
    "prettier:check": "prettier --check .",
    "prettier:lint": "prettier --write .",
    "coverage": "npx hardhat coverage --sources MarketplaceV1.sol",
    "node": "npx hardhat node",
    "deploy_local": "npx hardhat --network hardhat run scripts/000-deploy-marketplace.ts",
    "local": "npx run-p node deploy_local"
  },
  "devDependencies": {
    "@arbitrum/nitro-contracts": "^1.2.1",
